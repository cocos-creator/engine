{
    "$schema": "./modules-entry/schema.json",
    "modules": {
        "base": {
            "required": true,
            "default": true,
            "entries": [
                "base"
            ]
        },
        "gfx-webgl": {
            "required": true,
            "default": true,
            "entries": [
                "gfx-webgl"
            ]
        },
        "gfx-webgl2": {
            "default": true,
            "entries": [
                "gfx-webgl2"
            ]
        },
        "ui": {
            "default": true,
            "entries": [
                "ui"
            ]
        },
        "particle": {
            "default": true,
            "entries": [
                "particle"
            ]
        },
        "physics-cannon": {
            "default": true,
            "entries": [
                "physics-cannon",
                "physics-framework"
            ],
            "mutex": "physics-*"
        },
        "physics-ammo": {
            "entries": [
                "physics-ammo",
                "physics-framework",
                "wait-for-ammo-instantiation"
            ],
            "wechatPlugin": false,
            "mutex": "physics-*"
        },
        "physics-builtin": {
            "entries": [
                "physics-builtin",
                "physics-framework"
            ],
            "mutex": "physics-*"
        },
        "physics-2d-box2d": {
            "default": true,
            "entries": [
                "physics-2d-box2d",
                "physics-2d-framework"
            ],
            "mutex": "physics-2d-*"
        },
        "physics-2d-builtin": {
            "entries": [
                "physics-2d-builtin",
                "physics-2d-framework"
            ],
            "mutex": "physics-2d-*"
        },
        "intersection-2d": {
            "default": true,
            "entries": [
                "intersection-2d"
            ],
            "mutex": "physics-2d-*"
        },
        "audio": {
            "default": true,
            "entries": [
                "audio"
            ],
            "native": "USE_AUDIO"
        },
        "video": {
            "default": true,
            "entries": [
                "video"
            ],
            "native": "USE_VIDEO"
        },
        "terrain": {
            "default": true,
            "entries": [
                "terrain"
            ]
        },
        "webview": {
            "default": true,
            "entries": [
                "webview"
            ],
            "native": "USE_WEBVIEW"
        },
        "tween": {
            "default": true,
            "entries": [
                "tween"
            ]
        },
        "tiled-map": {
            "default": true,
            "entries": [
                "tiled-map"
            ]
        },
        "spine": {
            "default": true,
            "wechatPlugin": false,
            "entries": [
                "spine"
            ]
        },
        "dragonbones": {
            "default": true,
            "wechatPlugin": false,
            "entries": [
                "dragonbones"
            ]
        }
    }
}
